        //console.log(keyGroup, keyEncrypt)
        let divInitial = document.createElement('div')
        divInitial.setAttribute('class', 'col-md-12 border-top')
        divInitial.setAttribute('id', 'divGroup_' + keyPane + '_' + keyEncrypt)

